\begin_layout Standard
\begin_inset Box Frameless
position "t"
hor_pos "c"
has_inner_box 1
inner_pos "t"
use_parbox 0
use_makebox 0
width "100col%"
special "none"
height "1in"
height_special "totalheight"
thickness "0.4pt"
separation "3pt"
shadowsize "4pt"
framecolor "black"
backgroundcolor "none"
status open

\begin_layout Plain Layout

\family typewriter
01 Lower <- LowestIndex
\end_layout

\begin_layout Plain Layout

\family typewriter
02 Upper <- HighestIndex
\end_layout

\begin_layout Plain Layout

\family typewriter
03 REPEAT
\end_layout

\begin_layout Plain Layout

\family typewriter
04 
\begin_inset space \qquad{}
\end_inset

Middle <- (Lower + Upper) DIV 2
\end_layout

\begin_layout Plain Layout

\family typewriter
05 
\begin_inset space \qquad{}
\end_inset

IF SearchItem > Value[Middle]
\end_layout

\begin_layout Plain Layout

\family typewriter
O6 
\begin_inset space \qquad{}
\end_inset

THEN
\end_layout

\begin_layout Plain Layout

\family typewriter
07 
\begin_inset space \qquad{}
\end_inset


\begin_inset space \qquad{}
\end_inset

Lower <- Middle + l
\end_layout

\begin_layout Plain Layout

\family typewriter
08 
\begin_inset space \qquad{}
\end_inset

ELSE
\end_layout

\begin_layout Plain Layout

\family typewriter
09 
\begin_inset space \qquad{}
\end_inset


\begin_inset space \qquad{}
\end_inset

Upper <- Middle — l
\end_layout

\begin_layout Plain Layout

\family typewriter
10 
\begin_inset space \qquad{}
\end_inset

ENDIF
\end_layout

\begin_layout Plain Layout

\family typewriter
11 UNTIL Value[Middle] = SearchItem OR Lower > Upper
\end_layout

\begin_layout Plain Layout

\family typewriter
12 OUTPUT "Score found at position" Middle
\end_layout

\end_inset


\end_layout

