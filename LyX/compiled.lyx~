\family typewriter

\begin_inset space \qquad{}

\end_inset


\begin_inset space \qquad{}

\end_inset

Value <- 15
\family default

\end_layout

\begin_layout Plain Layout

\family typewriter

\begin_inset space \qquad{}

\end_inset

ELSEIF HexDigit IN (
\begin_inset Quotes qrd
\end_inset

0
\begin_inset Quotes qrd
\end_inset

, 
\begin_inset Quotes qld
\end_inset

l
\begin_inset Quotes qrd
\end_inset

, ..., 
\begin_inset Quotes qld
\end_inset

9
\begin_inset Quotes qrd
\end_inset

] THEN 
\family default

\end_layout

