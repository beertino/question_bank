\family typewriter

\begin_inset space \qquad{}

\end_inset


\begin_inset space \qquad{}

\end_inset

WHILE(ThisArray[High] > Pivot) //Scan right
\family default

\end_layout

\begin_layout Plain Layout

\family typewriter

\begin_inset space \qquad{}

\end_inset


\begin_inset space \qquad{}

\end_inset


\begin_inset space \qquad{}

\end_inset

