\family typewriter
04 
\begin_inset space \qquad{}

\end_inset

ENDIF 
\family default

\end_layout

\begin_layout Plain Layout

\family typewriter
05 
\begin_inset space \qquad{}

\end_inset

mid <- low + INT( (high — low) /2)
\family default

\end_layout

\begin_layout Plain Layout

\family typewriter
06 
\begin_inset space \qquad{}

\end_inset

