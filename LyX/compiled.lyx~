\begin_inset space \qquad{}

\end_inset

Answer ← Binomial(N – 1, R) + Binomial(N – 1, R – 1) 
\family default

\end_layout

\begin_layout Plain Layout

\family typewriter
07
\begin_inset space \qquad{}

\end_inset


\begin_inset space \qquad{}

\end_inset

ENDIF 
\family default

\end_layout

\begin_layout Plain Layout

\family typewriter
08
\begin_inset space \qquad{}

\end_inset


\begin_inset space \qquad{}

\end_inset

RETURN Answer 
\family default

\end_layout

\begin_layout Plain Layout

\family typewriter
09
\begin_inset space \qquad{}

\end_inset

ENDFUNCTION
\family default

\end_layout

\end_inset


\end_layout

