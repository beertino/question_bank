\family typewriter
13 
\begin_inset space \qquad{}

\end_inset


\begin_inset space \qquad{}

\end_inset


\begin_inset space \qquad{}

\end_inset

ENDIF 
\family default

\end_layout

\begin_layout Plain Layout

\family typewriter
14 
\begin_inset space \qquad{}

\end_inset


\begin_inset space \qquad{}

\end_inset


\begin_inset space \qquad{}

\end_inset

Current ← Next 
\family default

\end_layout

\begin_layout Plain Layout

\family typewriter
15 
\begin_inset space \qquad{}

\end_inset

UNTIL Current > LENGTH(Arr) 
\family default

\end_layout

\begin_layout Plain Layout

\family typewriter
16 
\begin_inset space \qquad{}

\end_inset

