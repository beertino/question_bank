backgroundcolor "none"
status open


\begin_layout Plain Layout

\family typewriter
lst1: 'A'->'B'->'C'->'D'->'E'
\family default

\end_layout

\begin_layout Plain Layout

\family typewriter
lst2 = split(lst1, 2)
\family default

\end_layout

\begin_layout Plain Layout

\family typewriter
lst1: 'A'->'B' 
\family default

\end_layout

\begin_layout Plain Layout

\family typewriter
lst2: 'C'->'D'->'E'
\family default

\end_layout

\end_inset


\end_layout

\end_deeper
\begin_layout Itemize

\family typewriter
join(llst1,llist2)
\family default
 This function joins 2 linked lists as one.
\end_layout

\begin_deeper
\begin_layout Standard
For example:
\end_layout

\begin_layout Standard
\noindent

