\family typewriter
100 
\begin_inset space \qquad{}
\end_inset

ENDIF 
\end_layout

\begin_layout Plain Layout

\family typewriter
110 
\begin_inset space \qquad{}
\end_inset

RETURN Result 
\end_layout

