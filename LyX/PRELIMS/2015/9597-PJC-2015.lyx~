\end_layout

\begin_layout Plain Layout

\family typewriter
\begin_inset space \qquad{}
\end_inset


\begin_inset space \qquad{}
\end_inset

Else Search(Mid+1, High) 
\end_layout

\begin_layout Plain Layout

\family typewriter
End Search 
\end_layout

